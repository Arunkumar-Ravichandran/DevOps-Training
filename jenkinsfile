agent any

stages {
    stage ('code-checkout'){
        steps{
            echo 'hello world'
        }

    }
    stage ('build stage'){
        steps{
            echo 'build success'
        }
    }
    stage ('test stage'){
        steps{
            echo 'test success'
        }
    }
    stage ('Deployment stage'){
        steps{
            echo 'deployment success'
        }
    }
}
}

